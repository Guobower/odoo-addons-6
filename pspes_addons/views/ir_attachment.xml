<?xml version="1.0" encoding="utf-8"?>

<odoo>
<data>

       <record model="ir.ui.view" id="pspes_ir_attachment_search">
        <field name="name">pspes.ir.attachment.search</field>
        <field name="model">ir.attachment</field>
        <field name="inherit_id" ref="base.view_attachment_search"/>
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="pspes_task_id"/>
                <field name="installation_id"/>
                <field name="condominium_id"/>
                <field name="officer_id"/>
            </field>
        </field>
    </record>


   <record model="ir.ui.view" id="pspes_ir_attachment_tree">
        <field name="name">pspes.ir.attachment.tree</field>
        <field name="model">ir.attachment</field>
        <field name="inherit_id" ref="base.view_attachment_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="replace">
                <tree>
                    <field name="name"/>
                    <field name="pspes_task_id"/>
                    <field name="officer_id"/>
                    <field name="condominium_id"/>
                    <field name="deadline"/>
                    <field name="create_date"/>
                    <field name="datas"/>
                </tree>
            </xpath>
        </field>
    </record>


</data>
</odoo>
